<form class="container" action="/api/dust/createAll" method="post">
    <div class="mb-3">
        <label class="form-label">데이터생성-한번에</label>
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>

<form class="container" action="/api/dust/selfupdate" method="post">
    <div class="mb-3">
        <label class="form-label">업데이트(전체 데이터지우고 다시생성)</label>
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>

<form class="container" action="/api/dust/list/search" method="get">
    <div class="mb-3">
        <label class="form-label">도시(데이터검색)</label>
        <td>
            <select id="div_apv_sq" class="form-control" name="cityName">
                <option value="서울">서울</option>
                <option value="부산">부산</option>
                <option value="대구">대구</option>
                <option value="인천">인천</option>
                <option value="광주">광주</option>
                <option value="광주">대전</option>
                <option value="광주">울산</option>
                <option value="광주">경기</option>
                <option value="광주">강원</option>
                <option value="광주">충북</option>
                <option value="광주">충남</option>
                <option value="광주">전북</option>
                <option value="광주">전남</option>
                <option value="광주">경북</option>
                <option value="광주">경남</option>
                <option value="광주">제주</option>
                <option value="광주">세종</option>
            </select>
        </td>
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">도시</th>
        <th scope="col">지역</th>
        <th scope="col">미세먼지 농도</th>
        <th scope="col">미세먼지 등급</th>
        <th scope="col">초미세먼지 농도</th>
        <th scope="col">초미세먼지 등급</th>
    </tr>
    </thead>
    <tbody>
    {{#dustList}}
        <tr>
            <th>{{id}}</th>
            <td>{{sido_name}}</td>
            <td>{{station_name}}</td>
            <td>{{pm10Value}}</td>
            <td>{{pm10Grade}}</td>
            <td>{{pm25Value}}</td>
            <td>{{pm25Grade}}</td>
        </tr>
    {{/dustList}}
    </tbody>
</table>